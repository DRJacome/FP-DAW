<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE instituto [
    <!ELEMENT instituto (ciclo+, alumno+)>
    <!ELEMENT ciclo EMPTY>
     <!ATTLIST ciclo
        codigo ID #REQUIRED
        nome NMTOKEN #REQUIRED
        grao (medio | superior) #IMPLIED>
    <!ELEMENT alumno EMPTY>
      <!ATTLIST alumno nome CDATA #REQUIRED>
      <!ATTLIST alumno ciclo IDREF #IMPLIED>
]>

